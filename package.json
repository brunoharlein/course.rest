    "mw.validation": "^1.0.15",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mockery": "^2.1.0",
    "nodemon": "^1.19.0",
    "parallelshell": "^3.0.2",
    "sinon": "^7.3.2",
    "sinon-bluebird": "^3.1.0",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "watch": "latest"
  },
  "main": "Sources/server.js",
